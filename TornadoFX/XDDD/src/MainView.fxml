<VBox alignment="CENTER" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="znacka" text="Label" textFill="#ff008c">
         <font>
            <Font name="Comic Sans MS" size="78.0" />
         </font></Label>
      <Button mnemonicParsing="false" onAction="#buttonPressed" text="Button">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin></Button>
   </children>
</VBox>
